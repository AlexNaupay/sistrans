##### Database config ... #####
database.driverClassName=com.mysql.jdbc.Driver
database.url=jdbc:mysql://localhost/DATABASE
database.username=USER
database.password=PASSWORD

##### System config #####
system.id=sistrans

##### Password config #####
user.password.max.length=30
user.password.min.length=8


##### File upload config #####
#max: 500MB
file.max.upload.size=524288000
